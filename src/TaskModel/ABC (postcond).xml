<taskModel about="urn:shayganfar.wpi.edu:models:ABC" 
           xmlns="http://www.cs.wpi.edu/~rich/cetask/cea-2018-ext"
           xmlns:disco="urn:disco.wpi.edu:Disco">

  <task id="A">
		<postcondition> $this.isComplete() </postcondition>
    <subtasks id="recipeA">
      <step name="step1" task ="B"/>
      <step name="step2" task="C"/>
      <step name="step3" task="D"/>
    </subtasks>
  </task>

  <task id="B">
		<postcondition> $this.isComplete() </postcondition>
    <subtasks id="recipeB">
      <step name="step1" task ="e"/>
      <step name="step2" task="F"/>
    </subtasks>
  </task>

  <task id="C">
		<postcondition> $this.isComplete() </postcondition>
    <subtasks id="recipeC">
      <step name="step1" task ="g"/>
      <step name="step2" task="h"/>
    </subtasks>
  </task>

  <task id="D">
		<postcondition> $this.isComplete() </postcondition>
    <subtasks id="recipeD">
      <step name="step1" task ="I"/>
      <step name="step2" task="J"/>
    </subtasks>
  </task>

  <task id="F">
		<postcondition> $this.isComplete() </postcondition>
    <subtasks id="recipeF">
      <step name="step1" task ="k"/>
    </subtasks>
  </task>

  <task id="I">
		<postcondition> $this.isComplete() </postcondition>
    <subtasks id="recipeI">
      <step name="step1" task ="l"/>
      <step name="step2" task ="m"/>
    </subtasks>
  </task>

  <task id="J">
		<postcondition> $this.isComplete() </postcondition>
    <subtasks id="recipeJ">
      <step name="step1" task ="n"/>
      <step name="step2" task ="o"/>
    </subtasks>
  </task>

  <task id="e"> <postcondition> $this.isOccurred() </postcondition> </task>
  <task id="k"> <postcondition> $this.isOccurred() </postcondition> </task>
  <task id="g"> <postcondition> $this.isOccurred() </postcondition> </task>
  <task id="h"> <postcondition> $this.isOccurred() </postcondition> </task>
  <task id="l"> <postcondition> $this.isOccurred() </postcondition> </task>
  <task id="m"> <postcondition> $this.isOccurred() </postcondition> </task>
  <task id="n"> <postcondition> $this.isOccurred() </postcondition> </task>
  <task id="o"> <postcondition> $this.isOccurred() </postcondition> </task>
  
</taskModel>
